{
  "name": "runner",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "install": "npm-run-all --parallel install-frontend install-backend --print-label",
    "start": "npm-run-all --parallel frontend backend --print-label",
    "frontend": "cd ./frontend & npm run dev",
    "install-frontend": "cd ./frontend & npm i",
    "backend": "cd ./backend & npm start",
    "install-backend": "cd ./backend & npm i"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
